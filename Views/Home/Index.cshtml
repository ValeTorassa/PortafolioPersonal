                                <p>3492 67-4205</p>
                            </div>

                        </div>

                    </div>
                    

                    <div class="col-lg-8 mt-5 mt-lg-0">
                        @using (Html.BeginForm("EnviarMail", "Email", FormMethod.Post))
                        {
                            <div class="row">
                                <div class="col-md-6 form-group">
                                    <input asp-for="Email.Name" class="form-control" id="name" placeholder="Tu nombre" required>
                                </div>
                                <div class="col-md-6 form-group mt-3 mt-md-0">
                                    <input asp-for="Email.Adress" class="form-control" id="email" placeholder="Tu Email" required>
                                </div>
                            </div>
                            <div class="form-group mt-3">
                                <input asp-for="Email.Subject" class="form-control" id="subject" placeholder="Asunto" required>
                            </div>
                            <div class="form-group mt-3">
                                <textarea asp-for="Email.Body" class="form-control" rows="5" placeholder="Mensaje" required></textarea>
                            </div>
                            <div class="my-3">
                                @if (TempData["SuccessMessage"] != null)
                                {
                                    <div class="alert alert-success" role="alert">
                                        @TempData["SuccessMessage"]
                                    </div>
                                }
                            </div>
                            <div class="text-center"><button type="submit">Enviar Mensaje</button></div>
                        }


                    </div>

                </div>

            </div>
        </section><!-- End Contact Section -->

